guild-front-end-color = #64c556

.guild-front-end-color
  color guild-front-end-color

#topbar .nav.nav-list-topbar li.guild-front-end-tab a:before,
#topbar .nav.nav-list-topbar li.active.guild-front-end-tab a:before
  background-color guild-front-end-color

.guild
  max-width 850px

  .guild-header span
    text-transform capitalize

  ul.guild-members
    list-style none
    margin 0
    padding 0

    li
      background-color #fff
      border 1px solid #fff
      border-radius 3px
      cursor pointer
      display inline-block
      margin 0 10px 20px
      padding 0 6px
      text-align center
      transition all 0.1s linear
      vertical-align top
      width 150px

      > img
        height 130px
        margin-top 6px
        width 100%

      > span.fa
        line-height 130px
        margin-top 6px

      h6
        line-height 1.3

      &:hover
        background-color #f5f5f5
        box-shadow 1px 1px 10px 1px rgba(0,0,0,0.3)